{
  "browserlists": [
    "defaults"
  ],
  "dependencies": {
    "flexsearch": "^0.7.43",
    "instant.page": "^5.2.0"
  },
  "devDependencies": {
    "@csstools/normalize.css": "^12.1.1",
    "autoprefixer": "^10.4.17",
    "bourbon": "^7.3.0",
    "cssnano": "^6.0.4",
    "eslint": "^8.56.0",
    "markdownlint-cli": "^0.39.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended-scss": "^8.0.0",
    "wait-on": "^7.2.0"
  },
  "name": "W210",
  "private": true,
  "scripts": {
    "build": "cd exampleSite && hugo --minify",
    "clean": "rm -rf public resources exampleSite/public exampleSite/resources",
    "dev": "npm-run-all --parallel start dev:*",
    "dev:browser": "wait-on http://localhost:1313 && open http://localhost:1313",
    "dev:sublime": "subl .",
    "lint": "npm-run-all --continue-on-error lint:*",
    "lint:markdown": "markdownlint *.md content/**/*.md",
    "lint:scripts": "eslint assets/js",
    "lint:styles": "stylelint \"assets/styles/**/*.{css,sass,scss,sss,less}\"",
    "prebuild": "npm run clean",
    "prestart": "npm run clean",
    "start": "cd exampleSite && hugo server --navigateToChanged",
    "test": "npm run lint"
  }
}
